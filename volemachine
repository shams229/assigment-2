#include <iostream>
#include <iomanip>
#include <vector>
#incl
#include <sstream>

using namespace std;
class Register
{
public:
    int value;
   
    {
    }
};

class Memory
{
public:
    vector<int> cells;

    Memory(int size) : cells(size, 0)
    {
    }

    int read(int address)
    {
        return cells[address];
    }

    void write(int address, int value)
    {
        cells[address] = value;
    }
};

    }
